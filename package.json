    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@storybook/addon-a11y": "^9.1.9",
    "@storybook/addon-docs": "^9.1.9",
    "@storybook/addon-vitest": "^9.1.9",
